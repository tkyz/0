#!/bin/bash

set -o errexit
set -o nounset
set -o pipefail

(

  mapfile -t ngwords << EOS
-----BEGIN PRIVATE KEY-----
-----BEGIN PRIVATE KEY-----
-----BEGIN RSA PRIVATE KEY-----
-----BEGIN PGP PRIVATE KEY BLOCK-----
-----BEGIN OPENSSH PRIVATE KEY-----
TODO:
${USER}
${HOME}
$(hostname)
$(hostname -d)
$(machine_id)
$(boot_id)
${GENESIS_HASH}
$(openpgp4fpr)
EOS

  # TODO: 一括
  for w in "${ngwords[@]}"; do
    # TODO: 自己参照
    git grep --cached --line-number --ignore-case -- "${w}" || true
  done

)
